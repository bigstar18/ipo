  <select id="selectByAll" resultMap="BaseResultMap" parameterType="map">
  	select <include refid="Base_Column_List" /> from (select t.*,rownum TNO from IPO_TRADETIME t) a where a.TNO between  #{page} and  #{rows}
  </select>
  
   <select id="selectAll" resultMap="BaseResultMap">
